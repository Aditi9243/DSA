class GfG{
	public void push(int a,Stack<Integer> s)
	{
            //add code here.
            s.push(a);
	}
	public int pop(Stack<Integer> s)
    {
            //add code here.
            return s.pop();
	}
	public int min(Stack<Integer> s)
    {
           //add code here.
           int min = s.pop();
           while(s.size()>0)
           {
               int ele = s.pop();
               if(ele < min)
                min = ele;
           }
           return min;
           
	}
	public boolean isFull(Stack<Integer>s, int n)
    {
           //add code here.
           return (s.size() == n);
	}
	public boolean isEmpty(Stack<Integer>s)
    {
           //add code here.
           return s.isEmpty();
	}
}